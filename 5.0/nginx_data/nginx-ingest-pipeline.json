{
    "description": "nginx pipeline",
    "processors": [{
        "rename": {
            "field": "agent",
            "target_field": "user_agent"
        }
    }, {
        "date": {
            "field": "time",
            "formats": ["dd/MMM/YYYY:HH:mm:ss Z"]
        }
    }, {
        "grok": {
            "field": "request",
            "patterns": ["%{WORD:request_action} %{DATA:request1} HTTP/%{NUMBER:http_version}"]
        }
    }]
}
